if int('0x2e0', 16)>int('0x3fb', 16):
    if int('0x2e0', 16) != int('0x559', 16):
        print(hex(int('0x2e0', 16) + int('0xa', 16)))
    else:
        print(hex(int('0x2e0', 16) - int('0xa', 16)))
else:
    if int('0x2e0', 16) != int('0x280', 16):
        print(hex(int('0x2e0', 16) - int('0xa', 16)))
    else:
        print(hex(int('0x2e0', 16) + int('0xa', 16)))





